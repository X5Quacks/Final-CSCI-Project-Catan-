import java.util.*;
import java.io.*;
/*
 * play game
 * please work
 */
public class CatanDriver{
  private Map<Node, Set<Edge>> roadPaths = new HashMap<>();
  private Map<Node, Set<Hexagon>> resources = new HashMap<>();
  
  public static void main(String[] args) throws IOException {
    Scanner input = new Scanner(new File("buildFile.txt"));
    for(int i = 0; i < 24; i++){
      String name = input.nextLine();
      Node e = new Node(name);
      roadPaths.put(e, new HashSet<Edge>());
      resources.put(e, new HashSet<Hexagon>());
      i++;
    }
  }
}
